(defn expand-button [props, details-id]
  (let [visible? @(rf/subscribe [:visible? details-id])]
    [:button (assoc props
                    :on-click (fn []
                                (rf/dispatch [:visible? details-id])))
     (if visible?
       unexpand-icon
