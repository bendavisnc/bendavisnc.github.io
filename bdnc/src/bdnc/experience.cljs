(defn main-section [props, details-id, company, role, logo]
  [:div.main-section props
   [:div {:class ["flex", "flex-col"]}
    [:div {:class ["text-5xl", "text-[#f9eac4]", "font-bold"]}
     company]
    [:div {:class ["font-light"]}
     role]
